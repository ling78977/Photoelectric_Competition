<?xml version="1.0"?>
<opencv_storage>

    <!-- hsv蓝色参数 -->
    <H_BLUE_MIN>0</H_BLUE_MIN>
    <H_BLUE_MAX>133</H_BLUE_MAX>
    <S_BLUE_MIN>64</S_BLUE_MIN>
    <S_BLUE_MAX>227</S_BLUE_MAX>
    <V_BLUE_MIN>63</V_BLUE_MIN>
    <V_BLUE_MAX>255</V_BLUE_MAX>

    <!-- hsv红色参数 -->
    <H_RED1_MIN>0</H_RED1_MIN>
    <H_RED1_MAX>10</H_RED1_MAX>
    <S_RED1_MIN>43</S_RED1_MIN>
    <S_RED1_MAX>255</S_RED1_MAX>
    <V_RED1_MIN>77</V_RED1_MIN>
    <V_RED1_MAX>255</V_RED1_MAX>

    <H_RED2_MIN>156</H_RED2_MIN>
    <H_RED2_MAX>180</H_RED2_MAX>
    <S_RED2_MIN>43</S_RED2_MIN>
    <S_RED2_MAX>255</S_RED2_MAX>
    <V_RED2_MIN>77</V_RED2_MIN>
    <V_RED2_MAX>255</V_RED2_MAX>

    <!-- 绿色hsv颜色参数 -->

    <H_GREEN_MIN>35</H_GREEN_MIN>
    <H_GREEN_MAX>99</H_GREEN_MAX>
    <S_GREEN_MIN>0</S_GREEN_MIN>
    <S_GREEN_MAX>255</S_GREEN_MAX>
    <V_GREEN_MIN>0</V_GREEN_MIN>
    <V_GREEN_MAX>255</V_GREEN_MAX>
    <!-- 黄色hls颜色参数 -->
    <H_YELLOW_MIN>0</H_YELLOW_MIN>
    <H_YELLOW_MAX>86</H_YELLOW_MAX>
    <L_YELLOW_MIN>44</L_YELLOW_MIN>
    <L_YELLOW_MAX>255</L_YELLOW_MAX>
    <S_YELLOW_MIN>0</S_YELLOW_MIN>
    <S_YELLOW_MAX>255</S_YELLOW_MAX>

    <!-- 0 RED
         1 BLUE
     -->
    <MY_COLOR>0</MY_COLOR>

    <TIME_OUT>100</TIME_OUT>
    <SAMPLE_TIMES>10</SAMPLE_TIMES>

    <!-- 0 不调试
         1 调试
     -->
    <HSV_EDIT>1</HSV_EDIT>
    <!-- 宝藏识别最小面积 -->
    <TREASURE_AREA_MIN>500</TREASURE_AREA_MIN>
    <!-- 宝藏识别长宽高比率阈值 -->
    <TREASURE_RATE_MIN>0.4</TREASURE_RATE_MIN>
    <TREASURE_RATE_MAX>0.75</TREASURE_RATE_MAX>

    <!-- 检测三角形 拟合近似精度 -->
    <JUDGEBOMP_RED>0.1</JUDGEBOMP_RED>
    <EPSILON>0.01</EPSILON>
    <APPROX>6</APPROX>
    <CIRCLE_RATE>0.8</CIRCLE_RATE>



    
    <!-- 形态学闭操作的卷积核大小 -->
    <MORPHOLOGRAY_KERNEL_SIZE_CLOSE_1>5</MORPHOLOGRAY_KERNEL_SIZE_CLOSE_1>
    <MORPHOLOGRAY_KERNEL_SIZE_CLOSE_2>5</MORPHOLOGRAY_KERNEL_SIZE_CLOSE_2>
    <!-- 形态学开操作的卷积核大小 -->
    <MORPHOLOGRAY_KERNEL_SIZE_OPEN_1>5</MORPHOLOGRAY_KERNEL_SIZE_OPEN_1>
    <MORPHOLOGRAY_KERNEL_SIZE_OPEN_2>5</MORPHOLOGRAY_KERNEL_SIZE_OPEN_2>

    <!-- 腐蚀时的卷积核大小 -->
    <KERNEL_SIZE_ERODE_1>5</KERNEL_SIZE_ERODE_1>
    <KERNEL_SIZE_ERODE_2>5</KERNEL_SIZE_ERODE_2>

    <!-- 膨胀时的卷积核大小 -->
    <KERNEL_SIZE_DILATE_1>3</KERNEL_SIZE_DILATE_1>
    <KERNEL_SIZE_DILATE_2>3</KERNEL_SIZE_DILATE_2>

    <!-- 腐蚀操作迭代次数 -->
    <ERODE_ITERATIONS>3</ERODE_ITERATIONS>

    <!-- 膨胀操作迭代次数 -->
    <DILATE_ITERATIONS>5</DILATE_ITERATIONS>
    <CLOSE_ITERATIONS>3</CLOSE_ITERATIONS>
    <VALUE_GRAY>30</VALUE_GRAY>



</opencv_storage>